doctype html
html(lang='en')
head
	block title
		title Home
	meta(charset='utf-8')
	meta(name='viewport', content='width=device-width, initial-scale=1.0')
	link(href='static/css/main.css' rel='stylesheet')
body
	#tracks_cont.cont
		h4 Tracks Stream
			a#streamBtn.btn(href='#') Start Stream
			span.keywords(contenteditable="true")
		.list

	#search_cont.cont
		h4 Search
			a#searchBtn.btn(href='#') Start Search
			span.keywords(contenteditable="true") html5 since:2013-11-11
		.list

	#userList_cont.cont
		h4 User list tweets
			a#userDefinedListBtn.btn(href='#') Start Search
		.list

	script(src='socket.io/socket.io.js')
	script(src='http://code.jquery.com/jquery-1.10.1.min.js')
	script(src='static/js/tweets.js')